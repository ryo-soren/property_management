
  <div>
  <h3>Your Question</h3>
  <%= form_with(model:[@property, @question]) do |f| %>  
    <div>
      <%= f.text_area :question, cols: 50, rows: 5, placeholder:"Whats your question about this property?" %>
    </div>
    <%= f.submit "Submit", class: "btn btn-outline-primary btn-size-larger my-2" %>
  <% end %>
  </div>
