<h3>Questions</h3>
 <% if @question.errors.any? %>
  <ul>
    <% @question.errors.full_messages.each do |msg| %>
      <li> <%= msg %></li>
    <% end %> 
  </ul>
<% end %> 

<%= form_with(url:[property_questions_path]) do |f| %>  
  <div>
    <%= f.text_area :body, cols: 50, rows: 5, placeholder:"Whats your question about this property?" %>
  </div>
  <%= f.submit "Submit", class: "btn btn-outline-primary my-2" %>
<% end %>